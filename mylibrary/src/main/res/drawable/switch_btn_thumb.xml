<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
            <corners android:radius="99dp" />
            <size android:width="99dp" android:height="99dp" />
        </shape>
    </item>
    <item android:state_checked="false" android:state_enabled="false">
        <shape android:shape="oval">
            <solid android:color="#FEFEFE" />
            <corners android:radius="99dp" />
            <stroke android:width="1px" android:color="#F0F0F0" />
            <size android:width="99dp" android:height="99dp" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
            <corners android:radius="99dp" />
            <size android:width="99dp" android:height="99dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#FEFEFE" />
            <corners android:radius="99dp" />
            <stroke android:width="2px" android:color="#19000000"/>
            <size android:width="99dp" android:height="99dp" />
        </shape>
    </item>
</selector>